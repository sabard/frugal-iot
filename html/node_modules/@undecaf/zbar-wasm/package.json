{
  "name": "@undecaf/zbar-wasm",
  "version": "0.9.16",
  "description": "A WebAssembly build of the C/C++ ZBar barcode reader",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "exports": {
    "require": "./dist/main.cjs",
    "script": "./dist/index.js",
    "default": "./dist/main.js"
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts",
    "dist/*.wasm",
    "dist/*.js.map"
  ],
  "types": "./dist/main.d.ts",
  "typings": "./dist/main.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/undecaf/zbar-wasm.git"
  },
  "keywords": [
    "zbar",
    "barcode",
    "qrcode",
    "scanner",
    "wasm",
    "webassembly",
    "emscripten"
  ],
  "author": "F. Kasper <fkasper@modus-operandi.at>",
  "license": "LGPL-2.1+",
  "bugs": {
    "url": "https://github.com/undecaf/zbar-wasm/issues"
  },
  "homepage": "https://github.com/undecaf/zbar-wasm#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^27.0.3",
    "canvas": "^2.9.3",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "rollup": "^2.77.2",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "jschardet": "^3.0.0"
  }
}
